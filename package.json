{
  "name": "Puppetmaster",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "dev:frontend": "vite",
    "dev:backend": "NODE_ENV=development node server.js",
    "debug:backend": "node --inspect=127.0.0.1:9229 server.js",
    "proxy": "mitmproxy -s ./proxy/ua_sniffer.py --listen-port 8082 --ssl-insecure",
    "build": "vite build",
    "preview": "vite preview",
    "start": "NODE_ENV=production node server.js",
    "test": "NODE_ENV=test jest --coverage",
    "lint": "eslint . --ext .js,.cjs",
    "dev": "concurrently -n FRONTEND,BACKEND -c magenta,blue \"npm run dev:frontend\" \"npm run dev:backend\"",
    "fix-ports": "kill $(lsof -t -i :3000 -i :8081 -i :8082) 2>/dev/null || echo 'No processes found'"
  },
  "devDependencies": {
    "@types/node": "^24.0.14",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "vite": "^6.3.0"
  },
  "dependencies": {
    "better-sqlite3": "^11.9.1",
    "chalk": "^5.4.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^7.1.0",
    "http-proxy": "^1.18.1",
    "json2csv": "^6.0.0-alpha.2",
    "morgan": "^1.10.0",
    "open": "^10.1.1",
    "puppeteer": "^24.6.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "socket.io": "4.8.1",
    "socket.io-client": "4.8.1",
    "sqlite": "^5.1.1",
    "tail": "^2.2.6",
    "user-agent-parser": "^0.6.0",
    "uuid": "^11.1.0"
  }
}
